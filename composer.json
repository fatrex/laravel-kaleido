{
    "name": "fatrex/laravel-kaleido",
    "description": "A schema manager for Laravel",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Fatrex\\LaravelKaleido\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fatrex\\LaravelKaleido\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fatrex\\LaravelKaleido\\LaravelKaleidoServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Daniele Lenares",
            "email": "daniele@lenares.me"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0|^12.0"
    },
    "require-dev": {
        "pestphp/pest": "4.x-dev",
        "orchestra/testbench": "10.x-dev",
        "pestphp/pest-plugin-laravel": "4.x-dev"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    }
}
